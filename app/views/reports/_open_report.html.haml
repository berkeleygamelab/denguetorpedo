.report.open_report
  %h4== Report #{open_report.id}: #{open_report.complete_address}
  .before_photo
    = image_tag open_report.before_photo(:thumb)
  .status
    Open
  .time_stamp
    = open_report.strftime_updated_at

  .report_content
    %p
      "
      = open_report.report
      %a "

    %a Reporter:
    %a.user==#{open_report.reporter.username unless open_report.reporter.nil?}
    %br
    %a Eliminator: Pending...







