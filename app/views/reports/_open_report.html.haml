.report.open_report
  %h4== Report #{open_report.id}: #{open_report.complete_address}
  .before_photo
    = image_tag open_report.before_photo(:thumb)
  .after_photo
    = image_tag open_report.after_photo(:thumb)
  .report_content
    = open_report.report
  .bottom_section
    .status
      Open
    %h4== Reporter: #{open_report.reporter.username unless open_report.reporter.nil?}
    %h4 Eliminator: Pending...
    .time_stamp
      = open_report.strftime_updated_at
