
    
    
    
.user_report.user_resolved_report
  .row
    .span8
      %h4== Report #{resolved.id}: #{resolved.complete_address}
    .span2.user_report_time
      = resolved.strftime_updated_at
      
  .row
    .span2.user_before_photo
      =image_tag 'http://placehold.it/100x100'
    .span2.user_after_photo
      =image_tag 'http://placehold.it/100x100'
    .span5
      .row
        .user_report_content.span5
          %p
            "
            = resolved.report            
            "
      .row.user_report_people
        Reporter:
        %a.user==#{resolved.reporter.username unless resolved.reporter.nil?}
        %br
        Eliminated by:
        %a.user==#{resolved.eliminator.username unless resolved.eliminator.nil?}
    .span1.user_resolved.status
      Resolved
  .row
    .user_report_options
      =link_to (image_tag 'icons/reports icon/star.png', :id=>'user_report_favorite'), '#'
      =link_to (image_tag 'icons/reports icon/delete2.png', :id=>'user_report_delete'), '#'




  
